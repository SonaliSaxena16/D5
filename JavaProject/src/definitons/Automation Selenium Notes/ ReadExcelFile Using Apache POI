
class ReadExcelFile{

	psvm(){
	File filePath = new File("path of the file")
	FileInputStream readingFile = new FileInputStream(filePath);
	Workbook wb = WorkbookFactory.create(readingFile);
	
	Sheet sh = wb.getSheet("Sheet1"); 
	Row r = sh.getRow(0);
	Cell c = r.getCell(0);
	String data = c.getStringCellValue();
	

}}